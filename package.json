{
  "name": "obraexpress-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev --port 3000",
    "dev:normal": "next dev",
    "dev:chrome": "start dev-chrome.bat",
    "build": "next build",
    "build:static": "cross-env STATIC_EXPORT=true NEXT_PUBLIC_AUTH_MODE=hostinger next build",
    "start": "next start",
    "lint": "next lint",
    "export": "npm run build:static",
    "deploy:hostinger": "npm run export && echo Deploy the 'out' folder to Hostinger",
    "analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "build:analyze": "npm run build && npm run analyze"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.3",
    "@react-three/fiber": "^9.3.0",
    "@supabase/supabase-js": "^2.55.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/recharts": "^1.8.29",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "daisyui": "^5.0.50",
    "framer-motion": "^12.23.12",
    "google-auth-library": "^10.3.0",
    "googleapis": "^157.0.0",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.1.2",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "three": "^0.179.1",
    "transbank-sdk": "^6.1.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
